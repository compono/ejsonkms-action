{
  "name": "ejson-action",
  "type": "module",
  "version": "0.3.0",
  "description": "Simple action that executes ejson command over a file",
  "scripts": {
    "lint": "npx eslint --fix .",
    "format": "npx prettier --write .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "pre-commit": "pre-commit install --hook-type commit-msg",
    "deps:upgrade": "node ./scripts/depsUpgrade.cjs",
    "deps:upgrade:force": "node ./scripts/depsUpgrade.cjs --force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Drafteame/sync-secrets-manager.git"
  },
  "keywords": [
    "github",
    "action",
    "json",
    "encrypt",
    "decrypt"
  ],
  "author": "Eduardo Aguilar <dante.aguilar@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Drafteame/ejson-action/issues"
  },
  "homepage": "https://github.com/Drafteame/ejson-action#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "child_process": "^1.0.2",
    "lodash": "^4.17.21",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@eslint-community/eslint-utils": "^4.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "sinon": "^18.0.0"
  }
}
